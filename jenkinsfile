#!groovy

pipeline {
  agent none
  stages {
   
    stage('Docker Build') {
      agent any
      steps {
        sh 'docker build -t mysql:latest .'
      }
}
     stage('Docker Run') {
      agent any
      steps {
        sh 'docker run -d -p 3306:3306 --name demo1 mysql:latest'
      }
     
    }
  }
}
