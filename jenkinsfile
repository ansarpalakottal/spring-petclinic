#!groovy

pipeline {
  agent none
  stages {
   
    stage('Docker Build') {
      agent any
      steps {
        sh 'docker build -t tomcat:latest .'
      }
}
     stage('Docker Run') {
      agent any
      steps {
        sh 'docker run -d -p 8083:8085 --name demo tomcat:latest'
      }
     
    }
  }
}
